<h1>Listing pick_up_points</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Address</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @pick_up_points.each do |pick_up_point| %>
  <tr>
    <td><%= pick_up_point.name %></td>
    <td><%= pick_up_point.address %></td>
    <td><%= link_to 'Show', pick_up_point %></td>
    <td><%= link_to 'Edit', edit_pick_up_point_path(pick_up_point) %></td>
    <td><%= link_to 'Destroy', pick_up_point, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Pick up point', new_pick_up_point_path %>
